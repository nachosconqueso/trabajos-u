Proceso sin_titulo
	escribir "sueldo basico" 
	leer sueldo
	escribir "ingrese horas extra"
	leer horasextra
	multiplicacion <- sueldo * 0.033
	multiplicacion2 <- multiplicacion * horasextra
	multiplicacion3 <- sueldo * 20
	dividir <- multiplicacion3/100
	trunc1 <- trunc (dividir)
	resta <- sueldo - trunc1
	suma <- sueldo + multiplicacion2
	escribir "sueld0 basico:"  sueldo
	escribir "sueldo con 20% de descuento:" resta
	escribir "sueldo total:" suma
FinProceso
